#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../optipng-bin@7.0.1/node_modules/optipng-bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../optipng-bin@7.0.1/node_modules/optipng-bin/cli.js" "$@"
fi
