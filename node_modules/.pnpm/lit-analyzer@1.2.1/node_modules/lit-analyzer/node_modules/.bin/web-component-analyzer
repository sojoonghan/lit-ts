#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kunstar/test_typescript/nestjs/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kunstar/test_typescript/nestjs/client/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../web-component-analyzer@1.1.6/node_modules/web-component-analyzer/cli.js" "$@"
else
  exec node  "$basedir/../../../../../web-component-analyzer@1.1.6/node_modules/web-component-analyzer/cli.js" "$@"
fi
