#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mozjpeg@7.1.1/node_modules/mozjpeg/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mozjpeg@7.1.1/node_modules/mozjpeg/cli.js" "$@"
fi
